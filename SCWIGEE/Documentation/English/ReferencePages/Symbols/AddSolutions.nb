Cell[BoxData[{
 RowBox[{
  RowBox[{"eqs", "=", 
   RowBox[{"WardEquations", "[", 
    RowBox[{"{", 
     RowBox[{
     "\"\<J\>\"", ",", "\"\<J\>\"", ",", "\"\<J\>\"", ",", 
      "\"\<\!\(\*OverscriptBox[\(\[Xi]\), \(_\)]\)\>\""}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"vars", "=", 
   RowBox[{"DeleteCases", "[", 
    RowBox[{
     RowBox[{"DeleteDuplicates", "[", 
      RowBox[{"Cases", "[", 
       RowBox[{"eqs", ",", 
        RowBox[{
         RowBox[{"g", "[", "__", "]"}], "[", "__", "]"}], ",", "All"}], "]"}],
       "]"}], ",", 
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Multiplet", "[", "1", "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "1", ",", "1", ",", "1"}], "}"}], "]"}], "]"}], 
        ",", "_", ",", "1"}], "]"}], "[", "__", "]"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"bb", ",", "mm"}], "}"}], "=", 
   RowBox[{"CoefficientArrays", "[", 
    RowBox[{"eqs", ",", "vars"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ansatz", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Multiplet", "[", "1", "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "1", ",", "1", ",", "1"}], "}"}], "]"}], "]"}], 
        ",", "i", ",", "1"}], "]"}], "->", 
      RowBox[{"Function", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"u", ",", "v"}], "}"}], ",", 
        RowBox[{"Evaluate", "[", 
         RowBox[{
          RowBox[{"Sum", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"i", "==", "1"}], "&&", 
                RowBox[{"j", "==", "0"}], "&&", 
                RowBox[{"k", "==", "0"}]}], ",", "1", ",", 
               RowBox[{"\[Alpha]", "[", 
                RowBox[{"i", ",", "j", ",", "k"}], "]"}]}], "]"}], 
             SuperscriptBox["u", "j"], 
             SuperscriptBox["v", "k"]}], ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", "0", ",", "1"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"k", ",", "0", ",", 
              RowBox[{"1", "-", "j"}]}], "}"}]}], "]"}], 
          RowBox[{"T", "[", 
           RowBox[{"u", ",", "v"}], "]"}]}], "]"}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "3"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"shouldBeZero", "=", 
   RowBox[{
    RowBox[{"NullSpace", "[", 
     RowBox[{"Transpose", "[", "mm", "]"}], "]"}], ".", "bb"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"coeffSol", "=", 
   RowBox[{"First", "@", 
    RowBox[{"Solve", "[", 
     RowBox[{"Thread", "[", 
      RowBox[{
       RowBox[{"Flatten", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Normal", "[", 
            RowBox[{"Flatten", "[", 
             RowBox[{
              RowBox[{"CoefficientArrays", "[", 
               RowBox[{
                RowBox[{"shouldBeZero", "/.", "ansatz"}], ",", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"T", "[", 
                   RowBox[{"u", ",", "v"}], "]"}], ",", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"Derivative", "[", 
                    RowBox[{"1", ",", "0"}], "]"}], "[", "T", "]"}], "[", 
                   RowBox[{"u", ",", "v"}], "]"}], ",", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"Derivative", "[", 
                    RowBox[{"0", ",", "1"}], "]"}], "[", "T", "]"}], "[", 
                   RowBox[{"u", ",", "v"}], "]"}]}], "}"}]}], "]"}], "[", 
              RowBox[{"[", "2", "]"}], "]"}], "]"}], "]"}], "/.", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"u", "->", "uu"}], ",", 
             RowBox[{"v", "->", "vv"}]}], "}"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"uu", ",", "2", ",", "5"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"vv", ",", "2", ",", "5"}], "}"}]}], "]"}], "]"}], "==", 
       "0"}], "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"sol", "=", 
  RowBox[{"Thread", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"g", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Multiplet", "[", "1", "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"{", 
              RowBox[{"1", ",", "1", ",", "1", ",", "1"}], "}"}], "]"}], 
            "]"}], ",", "i", ",", "1"}], "]"}], "[", 
